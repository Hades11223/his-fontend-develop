  "scripts": {
    "preinstall": "git log -1 --format=%cd --date=local > app.info && git log --oneline -n 1 >> app.info && git branch --show-current >> app.info && date >> app.info && cp app.info public/app.info && mv app.info src/assets/app.info",
    "start": "yarn preinstall && node scripts/start.js",
    "build": "node scripts/build.js",
    "build:develop": "env-cmd -f .env.staging node scripts/build.js",
    "build:demo:develop": "env-cmd -f .env.demo node scripts/build.js",
    "build:demo1:develop": "env-cmd -f .env.demo1 node scripts/build.js",
    "build:stable": "env-cmd -f .env.stable node scripts/build.js",
    "build:production": "env-cmd -f .env.production node scripts/build.js",
    "build:dongtam:stable": "env-cmd -f .env.dongtam.stable node scripts/build.js",
    "build:dongtam:production": "env-cmd -f .env.dongtam.production node scripts/build.js",
    "build:tttm:production": "env-cmd -f .env.tttm.production node scripts/build.js",
    "build:myrehab:stable": "env-cmd -f .env.myrehab.stable node scripts/build.js",
    "build:myrehab:production": "env-cmd -f .env.myrehab.production node scripts/build.js",
    "build:allen:production": "env-cmd -f .env.allen.production node scripts/build.js",
    "build:vnhsc:stable": "env-cmd -f .env.vnhsc.stable node scripts/build.js",
    "build:vnhsc:production": "env-cmd -f .env.vnhsc.production node scripts/build.js",
    "build:dkhn:stable": "env-cmd -f .env.dkhn.stable node scripts/build.js",
    "build:pkth:stable": "env-cmd -f .env.pkth.stable node scripts/build.js",
    "build:ctm:stable": "env-cmd -f .env.ctm.stable node scripts/build.js",
    "build:ctm:production": "env-cmd -f .env.ctm.production node scripts/build.js",
    "test": "node scripts/test.js",
    "pick": "git cherry-pick -x",
    "logall": "git log --all"
  },
  "resolutions": {
    "moment": "2.29.1",
    "react-error-overlay": "6.0.9",
    "pako": "2.1.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
